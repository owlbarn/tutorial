        testing.tex
        utilities.tex
        visualisation.tex
        zoo.tex))

(rule
  (targets aeos.html)
  (deps    (:md_file aeos/README.md))
  (action  (run otb-convert_md %{md_file} -t html -o %{targets})))

(rule
  (targets aeos.tex)
  (deps    (:md_file aeos/README.md))
  (action  (run otb-convert_md %{md_file} -t latex -o %{targets})))

