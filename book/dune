(alias
  (name html)
  (deps aeos.html
        algodiff.html
        architecture.html
        backend.html
        case-finance.html
        case-image-inception.html
        case-nst.html
        case-obj-detect.html
        case-recommender.html
        cgraph.html
        constant.html
        convention.html
        core-opt.html
        dataframe.html
        diffequation.html
        distributed.html
        introduction.html
        linear-algebra.html
        maths.html
        ndarray.html
